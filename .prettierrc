{
  "printWidth": 80,
  "tabWidth": 2,
  "singleQuote": true,
  "trailingComma": "es5",
  "semi": true,
  "jsxSingleQuote": false,
  "bracketSpacing": true,
  "arrowParens": "always",
  "proseWrap": "preserve",
  "jsxBracketSameLine": false,
  "endOfLine": "lf",
  "useTabs": false,
  "plugins": ["@prettier/plugin-ruby"],
  "rubySingleQuote": false,
  "rubyHashLabel": true
}
